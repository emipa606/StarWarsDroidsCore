<Defs>
  <!--Base-->
  <ThingDef Name="BodyPartDroidBase" Abstract="True">
    <description>A droid bodypart.</description>
    <thingClass>ThingWithComps</thingClass>
    <category>Item</category>
    <useHitPoints>true</useHitPoints>
    <selectable>true</selectable>
    <altitudeLayer>Item</altitudeLayer>
    <tickerType>Never</tickerType>
    <alwaysHaulable>true</alwaysHaulable>
    <isTechHediff>true</isTechHediff>
    <pathCost>10</pathCost>
    <tradeTags>
      <li>DroidParts</li>
    </tradeTags>
    <comps>
      <li Class="CompProperties_Forbiddable" />
    </comps>
    <techLevel>Spacer</techLevel>
    <thingCategories>
      <li>SW_Droid_BodyParts</li>
    </thingCategories>
    <graphicData>
      <texPath>Things/Item/Health/HealthItem</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>0.80</drawSize>
      <color>(100,100,100)</color>
    </graphicData>
    <statBases>
      <MarketValue>200</MarketValue>
      <Mass>2</Mass>
      <MaxHitPoints>50</MaxHitPoints>
      <Flammability>0.7</Flammability>
      <Beauty>-4</Beauty>
      <DeteriorationRate>2.0</DeteriorationRate>
    </statBases>
  </ThingDef>
  <ThingCategoryDef>
    <defName>SW_Droid_BodyParts</defName>
    <label>droid parts</label>
    <parent>BodyParts</parent>
  </ThingCategoryDef>
