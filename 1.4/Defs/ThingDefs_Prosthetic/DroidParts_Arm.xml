      <ThingDef>SW_Droid_Arm_Computer_Scomp_Link</ThingDef>
    </descriptionHyperlinks>
    <defaultLabelColor>(170, 210, 100)</defaultLabelColor>
    <comps>
      <li Class="HediffCompProperties_VerbGiver">
